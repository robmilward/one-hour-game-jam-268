function generate_coins()
	coins={}
 for i=0,10 do	 
	 _coin={
	  x=ceil(rnd(120)),
	  y=ceil(rnd(120))
	 }
	
	 add(coins, _coin)
	end
end

function generate_enemies()
 for i=0,10 do	
	 _enemy={
	  x=ceil(rnd(120)),
	  y=ceil(rnd(120))
	 }
	
	 add(enemies, _enemy)
	end
end

generate_coins()
generate_enemies()
