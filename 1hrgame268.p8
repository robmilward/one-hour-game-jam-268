function detect_collisions()
 pickup_coins()
 
 enemy_collide() 
 
 enter_home() 
end

function pickup_coins()
 for coin in all(coins) do
  if(colliding(coin, player, 5)) then
   sfx(0)
   del(coins, coin)
   player.coins=player.coins+1
  end
 end
end

function enemy_collide()
	for enemy in all(enemies) do
  if(colliding(enemy, player, 5)) then
   sfx(2)
   player.coins=0
   player.at_home=1
   player.x=home.x
   player.y=home.y
   player.lives=player.lives-1
  end
 end
end

function enter_home()
	if(colliding(player,home,10)) then
 	if(player.at_home == 0) then
  	sfx(1)
  	generate_coins()
 	end	
 	player.at_home=1
  if(player.coins > 0) then
	  home.coins=home.coins+player.coins
	  player.coins=0
  end
 else
  player.at_home=0
 end
end	
